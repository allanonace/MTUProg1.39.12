			<Parameter name="InstallationConfirmationStatus" display="Installation" log="true" interface="true" source="MemoryMap.InstallConfirmationStatus"/>
			<Parameter name="DcuId" display="DCU ID" log="true" interface="true" conditional="MemoryMap.InstallConfirmationNotSynced=false"/>
			<Parameter name="RSSI" display="RSSI" log="true" interface="true" source="MemoryMap.RSSIStatus" conditional="MemoryMap.InstallConfirmationNotSynced=false"/>
			<Parameter name="Deviation" display="Deviation" log="true" interface="false" conditional="MemoryMap.InstallConfirmationNotSynced=false"/>
			<Parameter name="MTU_DateTime" display="MtuGmtDateTime" log="true" interface="false" source="MemoryMap.MtuDatetime" conditional="MemoryMap.InstallConfirmationNotSynced=false"/>
			<Parameter name="Port" display="Port {0}" log="true" interface="true">
				<Parameter name="Description" display="Description" log="true" interface="true" source="Port.Description"/>
				<Parameter name="Status" display="Status" log="true" interface="true" conditional="MemoryMap.StatusFlag=false"/>
				<Parameter name="MeterType" display="Meter Type" log="true" interface="true" source="Meter.Display" conditional="MemoryMap.StatusFlag=true"/>
				<Parameter name="MeterTypeId" display="Meter Type ID" log="true" interface="false" source="Meter.Id" conditional="MemoryMap.StatusFlag=true"/>
				<Parameter name="NumberOfDials" display="Number Of Dials" log="true" interface="true" source="Meter.NumberOfDials" conditional="MemoryMap.StatusFlag=true + Port.IsForPulse=true + Meter.NumberOfDials gt -1"/>
				<Parameter name="DriveDialSize" display="Drive Dial Size" log="true" interface="true" source="Meter.DriveDialSize" conditional="MemoryMap.StatusFlag=true + Port.IsForPulse=true + Meter.NumberOfDials gt -1"/>
				<Parameter name="UnitOfMeasure" display="Unit Of Measure" log="true" interface="true" source="Meter.UnitOfMeasure" conditional="MemoryMap.StatusFlag=true + Port.IsForPulse=true"/>
				<!-- INIT - Ecoder -->
				<Parameter name="BackFlowState" display="Backflow State" log="true" interface="true" conditional="MemoryMap.StatusFlag=true + Mtu.Ecoder=true"/>
				<Parameter name="DaysOfNoFlow" display="Days of No Flow" log="true" interface="true" conditional="MemoryMap.StatusFlag=true + Mtu.Ecoder=true"/>
				<Parameter name="LeakDetection" display="Leak Detection" log="true" interface="true" conditional="MemoryMap.StatusFlag=true + Mtu.Ecoder=true"/>
				<Parameter name="DaysOfLeak" display="Days of Leak" log="true" interface="true" conditional="MemoryMap.StatusFlag=true + Mtu.Ecoder=true"/>
